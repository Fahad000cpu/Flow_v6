rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // This rule allows anyone with your database reference to view, edit,
      // and delete all data in your Firestore database. It is useful for getting
      // started, but make sure to secure your database correctly before
      // launching your app.
      //
      // Allow reads and writes for the next 30 days
      allow read, write: if request.time < timestamp.date(2024, 9, 8);
    }
  }
}
